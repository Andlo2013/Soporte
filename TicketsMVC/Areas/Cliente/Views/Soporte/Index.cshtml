@model IEnumerable<TicketsMVC.Areas._CommonModel.TicketsModel>

@{
    ViewBag.Title = "Index";
}

    @section styles {
       
        <link rel="stylesheet" href="~/Content/jquery-ui.css" />
        <link rel="stylesheet" href="~/Content/appStyle.css" />
       

        <!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
<link rel="stylesheet" href="~/Content/fileupload.css" />
<link rel="stylesheet" href="~/Content/fileupload-ui.css" />
    }

    <h2>Tickets</h2>

    <p>
        @Html.ActionLink("Nuevo Ticket", "Create")
    </p>

    @Html.Partial("~/Views/_common/_showErrors.cshtml")

    <p>Empresa: @ViewBag.Empresa</p>
    <p>Plan: @ViewBag.Plan</p>
    <p>Fecha Inicia: @ViewBag.Inicia</p>
    <p>Fecha Vence: @ViewBag.Termina</p>
    <p>Tiempo Plan: @ViewBag.Minutos</p>
    <p>Tiempo Consumido: @ViewBag.MinutosUTI</p>

    

    <div id="SoporteTable"></div>

    @section Scripts {

        <script src="http://code.jquery.com/ui/1.10.1/jquery-ui.js"></script>
        <!-- Include one of jTable styles. -->
        <link href="~/Scripts/jtable/themes/metro/blue/jtable.min.css" rel="stylesheet" type="text/css" />
        <!-- Include jTable script file. -->
        <script src="~/Scripts/jtable/jquery.jtableMOD.min.js" type="text/javascript"></script>
        
        <script src="~/Scripts/_jtSoporte.js" type="text/javascript"></script>


<!-- The jQuery UI widget factory, can be omitted if jQuery UI is already included -->
<script src="~/js/vendor/jquery.ui.widget.js"></script>
<!-- The basic File Upload plugin -->
<script src="~/js/jquery.fileupload.js"></script>
<!-- The File Upload processing plugin -->
<script src="~/js/jquery.fileupload-process.js"></script>
<script src="~/js/jquery.fileupload-validate.js"></script>
<script src="~/js/_loadFunction.js"></script>
               
}
