@model IEnumerable<TicketsMVC.Areas._CommonModel.TicketsModel>

@{
    ViewBag.Title = "Index";
}

    @section styles {
        <link rel="stylesheet" href="~/Content/appStyle.css" />
    }

    <h2>Tickets</h2>

    <p>
        @Html.ActionLink("Nuevo Ticket", "Create")
    </p>

    @Html.Partial("~/Views/_common/_showErrors.cshtml")

    <p>Empresa: @ViewBag.Empresa</p>
    <p>Plan: @ViewBag.Plan</p>
    <p>Fecha Inicia: @ViewBag.Inicia</p>
    <p>Fecha Vence: @ViewBag.Termina</p>
    <p>Tiempo Plan: @ViewBag.Minutos</p>
    <p>Tiempo Consumido: @ViewBag.MinutosUTI</p>

    <div id="SoporteTable"></div>

    @section Scripts {

        <script src="http://code.jquery.com/ui/1.10.1/jquery-ui.js"></script>
        <!-- Include one of jTable styles. -->
        <link href="~/Scripts/jtable/themes/metro/blue/jtable.min.css" rel="stylesheet" type="text/css" />
        <!-- Include jTable script file. -->
        <script src="~/Scripts/jtable/jquery.jtable.min.js" type="text/javascript"></script>
        <script src="~/Scripts/_jtSoporte.js" type="text/javascript"></script>
    }
